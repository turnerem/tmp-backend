  "dependencies": {
    "express": "^4.17.3",
    "knex": "^0.20.8",
    "pg": "^7.17.1",
    "psql": "0.0.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "mocha": "^7.0.0",
    "chai": "^4.2.0",
    "supertest": "^4.0.2"
  },
